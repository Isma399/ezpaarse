{
  "EZPAARSE_ADMIN_MAIL": "ezpaarse@couperin.org",
  "EZPAARSE_ENV": "production",
  "EZPAARSE_NODEJS_PORT": "59599",
  "EZPAARSE_NODEJS_VERSION": "0.10.10",
  "EZPAARSE_LOG_FOLDER": "test/dataset",
  "EZPAARSE_OUTPUT_FIELDS":
  [
    "domain",
    "rtype",
    "mime",
    "doi",
    "issn",
    "eissn",
    "title",
    "platform",
    "unitid"
  ],
  "EZPAARSE_QUALIFYING_LEVEL": 1,
  "EZPAARSE_QUALIFYING_FACTORS": {
    "internal": {
      "rtype": 0.5,
      "mime": 0.5
    },
    "external": [
      {
        "file": "platforms/rid.json",
        "attribute": "code",
        "weight": 1
      }
    ]
  },
  "EZPAARSE_BIG_RATE": 900,
  "EZPAARSE_BIG_DURATION": 60,
  "EZPAARSE_TMP_CYCLE": 60,
  "EZPAARSE_TMP_LIFETIME":
  {
    "years":   0,
    "months":  0,
    "days":    1,
    "hours":   0,
    "minutes": 0
  },
  "EZPAARSE_IGNORED_DOMAINS":
  [
    "www.google.fr",
    "www.google.com"
  ]
}
