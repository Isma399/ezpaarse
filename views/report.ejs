<% include header %>

<% if (report) { %>
  <h1>Rapport d'ex√©cution</h1>
  <table class="table table-bordered table-hover">
  <% for (var key in report) { %>
    <tr>
      <td><%= key %></td>
      <% if (/^http/.test(report[key])) { %>
        <td><a href="<%= report[key] %>"><%= report[key] %></a></td>
      <% } else { %>
        <td><%= report[key] %></td>
      <% } %>
    </tr>
  <% } %>
  </table>
<% } else { %>
  <p>Le rapport que vous recherchez n'existe pas.</p>
<% } %>

<% include footer %>